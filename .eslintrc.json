{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb","prettier"
    ],
    "plugins": [
        "prettier"
    ],
    "globals": {
        "Atomics":"readonly",
        "SharedArrayBuffer":"readonly"
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "prettier/prettier":"error",
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
    }
}
